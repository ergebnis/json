includes:
	- phpstan-baseline.neon

parameters:
	ergebnis:
		noExtends:
			classesAllowedToBeExtended:
				- Ergebnis\DataProvider\AbstractProvider
				- InvalidArgumentException

	inferPrivatePropertyTypeFromConstructor: true

	level: max

	paths:
		- src/
		- test/
		- .php-cs-fixer.php
		- rector.php

	tmpDir: .build/phpstan/
